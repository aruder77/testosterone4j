package de.msg.xt.mdt.tdsl.swtbot {
	
	import de.msg.xt.mdt.tdsl.basictypes.*
	
	sut Stdtoolkit using controls { 
		TextControl, TreeControl, Label
	} 
	
	
	
	control TextControl {
		op setText(String str)
		op String getText
	}

	control TreeControl {
		op selectNode(String nodePath)
		op doubleClickNode(String nodePath)
	}
	
	control Label {
		op String getText
	}

	
}